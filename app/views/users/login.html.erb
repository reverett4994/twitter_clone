<h1>Log In!</h1>

<%= form_tag('login_user',method: "get") do %>
  <div class="field">
    <%= label_tag :username %><br>
    <%= text_field_tag :username %>
  </div>
  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>
    <%= submit_tag 'Log In' %>
<% end %>
